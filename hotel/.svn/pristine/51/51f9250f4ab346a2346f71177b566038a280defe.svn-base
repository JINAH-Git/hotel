@charset "EUC-KR";

.footer
{
	width: 100%;
	height: 170px;
	background-color: #F1E3C4;
}

.localBox
{
	width: 1200px;
	height: 170px;
	margin: 0 auto;
	padding: 0;
	font-size: 12px;
    color: #101010;
}

.brandInfo
{
	text-align: center;
	width: 1030px;
	height: 75px;
    display: block;
    overflow: hidden;
    float: right;
    box-sizing: border-box;
}

.info
{
	text-align: center;
	width: 1030px;
	height: 75px;
    display: block;
    overflow: hidden;
    box-sizing: border-box;
}

.brandInfo dt, .introduction dd
{
	display: inline-block;
	margin: 0 10px;
}

