@charset "EUC-KR";

ul, ol, li, body
{
	list-style: none;
	padding: 0;
	margin: 0; 
}

.head
{
 	width: 100%;
 	height: 120px;
 	position: relative;
 	z-index: 100;
 	background-color: #F1E3C4;
 	margin: 0;
 	padding: 0;
 	
}

.head a
{
	text-decoration: none;
	color: #101010;
}

.header
{
	width: 1200px; 
	height: 90px;
	position: relative;
	z-index: 100;
	display: block; 
	margin: 0 auto;
}

.logo
{
	width: 260px;
	font-size: 24px;
	margin: 0 auto;
}

.loginArea
{
	position: absolute; 
	top: 10px; 
	right: 10px; 
	font-size: 12px;
}

.hubMenu
{
	float: right;
}

.hubMenu li
{
	position: relative;
	display: inline-block;
	padding: 5px 10px;
	font-size: 16px;
	font-weight: 900;
}
