@charset "EUC-KR";

.menubar
{
	width: 270px; 
	height: 300px; 
	margin-top: 100px; 
	background-color: #F1E3C4;
	text-align: center;
}

.menubar h2
{	
	padding-top: 20px;
	color: brown;
}

.menubar hr
{
	height: 10px; 
	background-color: gold;
	border: none;
}

.menu
{
	width: 270px;
	height: 200px;
}

.menu li
{
	margin-top: 25px;
}

.menu a
{
	text-decoration: none;
	color: brown;
	font-size: 16px;
	font-weight: 900;
}