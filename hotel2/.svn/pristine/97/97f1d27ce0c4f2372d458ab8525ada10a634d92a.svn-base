@charset "EUC-KR";

.room
{
	position: relative;
	z-index: 1;
}

.btn_open
{
	width: 370px; 
	height: 220px;
	border-radius: 10%;
	text-align: center;
	font-size: 22px;
	font-weight: 700;
	opacity: 0.8;
	color: #fff;
	display: inline-block;
	margin: 20px 30px 10px;
}

.btn_open:hover
{
	opacity: 1;
	cursor: pointer;
}
