@charset "EUC-KR";

.service
{
	position: relative;
	min-height: 1000px;
}

.img
{
	width: 600px;
	height: 300px;
	margin: 0 auto;
}

.info
{
	width: 600px;
	margin: 0 auto;
	padding: 10px 0;
}

.adminBtn
{
	width: 600px;
	margin: 0 auto;
}


.service h4
{
	margin-top: 50px;
	margin-left: 120px;
	color: brown;
}

#fname1, #fname2, #fname3, #fname4
{
	margin-top: 10px;
	margin-left: 120px;
}

.btn
{
	height: 30px;
	position: absolute;
	right: 90px;
	margin-top: 10px;
}

#write, #cancel, #modify, #delete
{
	width: 60px;
	border-radius: 5%;
	border-width: 0;
	background: #DFDFE3;
	font-weight: 500;
	font-size: 18px;
	cursor: pointer;
}

