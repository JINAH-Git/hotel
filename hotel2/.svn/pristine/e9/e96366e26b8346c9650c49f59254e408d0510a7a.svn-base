@charset "EUC-KR";

#writeFrm
{
	position: relative;
	margin: 0 auto;
}

.writetb
{
	border: 3px solid #F1E3C4;
	width: 700px;
	min-height: 500px;
	margin: 60px auto 0;
}

.writetb hr
{
	border: 1px solid #F1E3C4;	
}

.writetb input
{
	border-top: none;
	border-bottom: 3px solid #F1E3C4;
	border-left: none;
	border-right: none;
	width: 100%; 
	height: 30px; 
	outline: none;
	box-sizing: border-box;
	
}

.writetb textarea
{
	border: none;
	width: 100%; 
	outline: none;
	box-sizing: border-box;
	resize: none;
}

#fname
{
	position: absolute;
	left: 90px;
	margin-top: 10px;	
}

.btn
{	
	width: 700px;
	height: 30px;
	margin: 10px auto 0;
}

.view_inner
{
	width:700px;
	min-height:600px;
	margin:85px;
}
.list_btn
{
	position:relative;
	width:70px;
	height:45px;
	font-size:18px;
}
.modify_btn
{
	position:relative;
	width:70px;
	height:45px;
	font-size:18px;
}
.delete_btn
{
	position:relative;
	width:70px;
	height:45px;
	font-size:18px;
}

.btn_area
{
	position:relative;
	top:30px;
}
.writetbl
{
	border-color:#66575A;
	border-width:2px 0px 2px 0px;
	border-style:solid;
}
.view_inner hr
{
	border-color:#EFEFEF;
	width:705px;
	margin-top:0px;
}
